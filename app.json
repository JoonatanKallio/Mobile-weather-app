{
  "expo": {
    "name": "weather_app",
    "slug": "weather-application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.joonatan_k.weatherapplication"
    },
    "scheme": "weather-application",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-router",
      ["expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow WeatherNow to use your location."
        }]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "55e02988-679d-4191-ace3-cfc7fb5a62bf"
      }
    },
    "owner": "joonatan_k"
  }
}
